FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue T 90
);

regions
(
    boxToCell
    {
        box (5e-3 4e-3 0.1e-3) (15e-3 11e-3 0.1e-3);
        fieldValues
        (
            volScalarFieldValue T 110
        );
    }
);


