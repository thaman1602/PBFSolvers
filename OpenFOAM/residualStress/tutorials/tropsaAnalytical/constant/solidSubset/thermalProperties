FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      thermalProperties;
    }
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

thermal
{
    type     constant;
    alpha    alpha [0 0 0 -1 0 0 0] 100e-6; //1.0e-5;
    T0       T0 [0 0 0 1 0 0 0] 0;

    // Data not used
    C        C [0 2 -2 -1 0 0 0] 0;
    k        k [1 1 -3 -1 0 0 0] 0;
    rho      rho [1 -3 0 0 0 0 0] 0;
    gaps     ();
    sources  ();
}


