    {
        nu0             nu0 [ 0 2 -1 0 0 0 0 ] 0.1;
        nuInf           nuInf [ 0 2 -1 0 0 0 0 ] 0.1;
        m               m [ 0 0 1 0 0 0 0 ] 1;
        n               n [ 0 0 0 0 0 0 0 ] 0;
    }
